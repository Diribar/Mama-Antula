<!-- Imagen izquierda -->
<div id="izquierda" class="contenedorImagen">
	<img src="<%= imagenes[0] %>">
</div>

<!-- Centro -->
<form id="formContactanos">
	<input name="nombre" type="text" autofocus placeholder="Nombre *">
	<input name="email" type="text" placeholder="Correo electrónico *" autocomplete="email">
	<input name="institucion" type="text" placeholder="Institución">
	<p class="comentario">Si pertenecés a alguna institución, ¿nos podés decir a cuál?</p>
	<textarea name="mensaje" placeholder="Mensaje *"></textarea>
	<p class="comentario">* Campos de contenido obligatorio</p>
	<p id="espera" class="ocultar">Esperá un momento por favor...</p>

	<button type="submit">Enviar</button>
</form>

<!-- Imagen derecha -->
<div id="derecha" class="contenedorImagen">
	<img src="<%= imagenes[1] %>">
</div>
