<!-- Carrusel -->
<div class="bloque bloque-<%= contenido.layout.codigo %> flexColAlign<%- locals.cambioStatusCont ? (" " +  contenido.statusRegistro.codigo) : '' %>">
	<% if (locals.cambioStatusCont && contenido.statusRegistro_id != aprobado_id) { %>
		<!-- Aprueba / Rechaza -->
		<%- include("REV-9ApruebaRech") %>

		<!-- Imagen del usuario que sugiriÃ³ el cambio de status -->
		<%- include("REV-9ImgUsuario", {usuario: contenido.statusSugeridoPor}) %>
	<% } %>

	<!-- Carrousel -->
	<div class="contenedorImgs flexRow" style="--anchorName: --carrusel-<%= contenido.id %>;">
		<% for (const carrusel of contenido.carrusel) { %>
			<!-- Bloque de imagen -->
			<div class="contenedorImg flexColAlign">
				<img src="/imgsEditables/<%- contenido.statusRegistro_id == creado_id ? '2-Revisar' : '1-Final' %>/<%= carrusel.imagen %>">
			</div>
		<% } %>
	</div>
	<% if (contenido.leyenda) { %>
		<!-- Leyenda -->
		<div class="leyenda"><%= contenido.leyenda %></div>
	<% } %>
</div>
