<!-- Estampa -->
<div class="bloque bloque-<%= contenido.layout.codigo %> flexCol <%- locals.cambioContenido ? contenido.statusRegistro.codigo : '' %>">
	<% if (locals.cambioContenido && contenido.statusRegistro_id != aprobado_id) { %>
		<!-- Aprueba / Rechaza -->
		<%- include("REV-9ApruebaRech") %>
		<% const {statusSugeridoPor} = contenido %>
		<% const {imagen, nombreCompleto} = statusSugeridoPor %>
		<%- include("REV-9ImgUsuario", {imagenUsuario: imagen, nombreCompleto: nombreCompleto}) %>
	<% } %>

	<% if (contenido.titulo) { %>
		<h3 class="titulo"><%= contenido.titulo %></h3>
	<% } %>
	<div class="imagenes flexRow">
		<img src="/imgsEditables/<%= subCarpeta %>/<%= contenido.imagen %>">
		<img src="/imgsEditables/<%= subCarpeta %>/<%= contenido.imagen2 %>">
	</div>
</div>
