<!-- Ficha técnica -->
<div id="fichaTecnica">
	<% if (encabezado.fechaEvento) { %>
		<h4>Ficha técnica</h4>
		<ul type="disc">
			<li>Fecha: <%= comp.fechaHora.diaMesAnoUTC(encabezado.fechaEvento) %></li>
			<li>Lugar: <%= encabezado.lugarExper.nombre %></li>
		</ul>
	<% } %>
</div>
