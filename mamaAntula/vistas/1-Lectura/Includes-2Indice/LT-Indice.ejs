<div id="indice" class="contenedor">
	<% for (const indice of indices) { %>
		<% const encabsIndice = encabezados.filter((n) => n.fechaEvento >= indice.fechaDesde && n.fechaEvento <= indice.fechaHasta); %>

		<% if (!encabsIndice.length) continue; %>

		<% if (encabsIndice.length == 1) { %>
			<!-- Indice con un solo encabezado -->
			<a href="?id=<%= encabsIndice[0].id %>" class="titulo" data-id="<%= encabsIndice[0].id %>">
				<%= indice.nombre %>
			</a>

		<% } else { %>
			<!-- Ãndice con varios encabezados -->
			<div class="tituloEncabs">
				<!-- Titulo -->
				<div class="titulo">
					<i class="fa-solid fa-caret-right mostrar"></i>
					<%= indice.nombre %>
				</div>

				<!-- Encabezados -->
				<div class="encabezados ocultar">
					<ul>
						<% for (const encabIndice of encabsIndice) { %>
							<li>
								<a href="?id=<%= encabIndice.id %>" class="encabezado" data-id="<%= encabIndice.id %>">
									<%= encabIndice.tituloElab %>
								</a>
							</li>
						<% } %>
					</ul>
				</div>

			</div>
		<% } %>
	<% } %>
</div>
