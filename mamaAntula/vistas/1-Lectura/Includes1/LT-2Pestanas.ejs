<nav class="listadoPestanas" data-tema_codigo="<%= temaActual.codigo %>">
	<% pestanasTema.forEach((pestana, i) => { %>
		<a
			href="/<%= seccionActual.url %>/<%= temaActual.url %>/<%= pestana.url %>"
			class="pestana<% if (pestana.id == pestanaActual.id) { %> activo<% } %>"
			data-pestana_codigo="<%= pestana.codigo %>"
		>
			<%= pestana.titulo %>
		</a>
	<% }) %>
</nav>
