<h1 id="tituloPrincipal"><%= tituloPagina %></h1>

<section id="cuerpo">
	<!-- Imagen izquierda -->
	<div id="izquierda" class="contenedorImagen">
		<img src="<%= imagenes[0] %>">
	</div>

	<% if (codigoVista == "altaOlvido") { %><%- include("US-1AltaOlvido") %><% } %>
	<% if (codigoVista == "edicion") { %><%- include("US-2Edicion") %><% } %>
	<% if (codigoVista == "cambioRol") { %><%- include("US-3CambioRol") %><% } %>

	<!-- Imagen derecha -->
	<div id="derecha" class="contenedorImagen">
		<img src="<%= imagenes[1] %>">
	</div>
</section>
