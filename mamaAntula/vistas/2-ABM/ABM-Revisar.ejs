<h2><%= seccionTema %></h2>
<% if (encabezado.titulo) { %>
	<!-- TÃ­tulo del contenido -->
	<h2 id="tituloDelContenido"><%= encabezado.titulo %></h2>
<% } %>

<!-- Contenido -->
<div id="contenido">
	<% if (encabezado) for (const contenido of encabezado.contenidos) { %>
