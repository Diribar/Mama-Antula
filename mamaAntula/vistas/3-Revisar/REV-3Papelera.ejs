<h1 id="tituloPrincipal"><%= tituloPagina %></h1>

<section id="cuerpo">
	<ul>
		<% for (const ruta in rutas) { %>
			<% const {encabezados} = rutas[ruta] %>
			<% if (encabezados.length > 1) { %>
				<!-- Ruta con título -->
				<li class="primario"><%= ruta %></li>
				<!-- Título -->
				<ul>
					<% for (const encabezado of rutas[ruta].encabezados) { %>
						<% const {titulo, tituloElab, anchor} = encabezado %>
						<li class="secundario"><span class="alinearVertical">
							<span class="icono">✅</span>
							<a href="<%= anchor %>"><%= titulo || tituloElab %></a>
						</span></li>
					<% } %>
				</ul>
			<% } else { %>
				<!-- Ruta sin título -->
				<li class="primario"><span class="alinearVertical">
					<span class="icono">✅</span>
					<a href="<%= rutas[ruta].encabezados[0].anchor %>"><%= ruta %></a>
				</span></li>
			<% } %>
		<% } %>
	</ul>
</section>
