<h1 id="tituloTema"><%= tituloPagina %></h1>

<section id="contenidoTemas">
	<!-- Imagen izquierda -->
	<div id="izquierda" class="contenedorImagen">
		<img src="<%= imagenes[0] %>">
	</div>

	<!-- Formulario -->
	<form id="formAltaOlvido">
		<input type="email" name="email" placeholder="Escribí tu mail" autofocus>
		<p id="mensaje"></p>
		<div id="fondoConfirma">
			<button type="submit" id="confirma">Envío de nueva contraseña</button>
		</div>
	</form>

	<!-- Imagen derecha -->
	<div id="derecha" class="contenedorImagen">
		<img src="<%= imagenes[1] %>">
	</div>
</section>
