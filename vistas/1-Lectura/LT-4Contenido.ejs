<% for (const contenido of contenidosFiltro) { %>
	<% if (contenido.texto && contenido.imagen) { %><div class="textoImagen"><p><%= contenido.texto %></p><%- include("LT-Imagen", {contenido}) %></div>
	<% } else if (contenido.texto) { %><p><%= contenido.texto %></p>
	<% } else if (contenido.imagen) { %><%- include("LT-Imagen", {contenido}) %>
	<% } else if (contenido.link) { %><%- include("LT-VideoYT") %>
	<% } else { %><%- include("LT-Carrousel") %>
	<% } %>
<% } %>
