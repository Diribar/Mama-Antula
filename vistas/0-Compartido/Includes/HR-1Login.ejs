<!-- Ícono -->
<i id="iconoUsuario" class="fa-regular <%= iconos.login %> mostrarClick pointer" title="Login/Registrarse"></i>

<!-- Form login -->
<form id="menuLogin" class="mostrarToggle ocultar">
	<!-- Inputs -->
	<input type="email" name="email" placeholder="e-mail">
	<div id="contContrasena" class="relative">
		<input type="password" name="contrasena" placeholder="contraseña">
		<i id="eye" class="<%= iconos.faSolid %> fa-eye-slash absolute"></i>
	</div>
	<p id="mensajeError"></p>


	<!--Iconos-->
	<div id="iconos">
		<i id="confirmaLogin" class="<%= iconos.faSolid %> <%= iconos.check %> scaleHover" title="Login"></i>
		<a href="/usuarios<%= rutasUsuario.altaOlvido %>">
			<i id="altaOlvido" class="<%= iconos.faSolid %> <%= iconos.altaUser %> scaleHover" title="Registrarte / Olvido de contraseña"></i>
		</a>
	</div>
</form>
