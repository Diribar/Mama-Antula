<ul type="none" class="listadoPestanas" data-tema_codigo="<%= tema.codigo %>">
	<% tema.pestanas.forEach((pestana, i) => { %>
		<li class="pestana<% if (pestanaActual ? pestana.id == pestanaActual.id : !i) { %> activo<% } %>" data-pestana_codigo="<%= pestana.codigo %>"><%= pestana.titulo %></li>
	<% }) %>
</ul>
