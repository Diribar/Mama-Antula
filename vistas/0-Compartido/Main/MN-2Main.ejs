<section id="tituloTema" class="flexRowAlign">
	<h1><i class="fa-solid fa-bars"></i></h1>

	<% tema = temasSeccion[0] %>
	<h1 id="titulo"><%= tema.titulo %></h1>
</section>

<% if (tema.pestanas.length) { %>
	<ul type="none" id="pestanas" class="flexRow">
		<% tema.pestanas.forEach((pestana, i) => { %>
			<li data-pestana="<%= pestana.id %>" <% if (!i) { %>class="activo"<% } %>><%= pestana.titulo %></li>
		<% }) %>
	</ul>
	<div id="contenidoPestana"></div>
<% } %>
