<section id="izquierda" class="flexRowAlign">

	<a href="/" id="tituloSitio">Familia Mama Antula</a>

	<div id="secciones" class="flexRowAlign">
		<% for (const seccion of secciones) { %>
			<a href="<%= seccion.link %>" <% if (seccion.nombre == locals.tituloPagina) { %> class="resaltar"<% } %>><%= seccion.nombre %></a>
		<% } %>
	</div>
</section>

<!-- Íconos de la derecha -->
<section id="derecha">
	<% if (!locals.usuario) { %><%- include("HR-1Login") %>
	<% } else { %><%- include("HR-2Logout") %>
	<% } %>
</section>
